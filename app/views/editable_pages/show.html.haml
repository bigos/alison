- @title = @editable_page.window_title
%p#notice= notice

- if current_admin
  %p
    %b Window title:
    = @editable_page.window_title
  %p
    %b Navbar title:
    = @editable_page.navbar_title
  %p
    %b Navbar index:
    = @editable_page.navbar_index
%p
  %b Content:
  != @editable_page.content

- if current_admin
  = link_to 'Edit', edit_editable_page_path(@editable_page)
  |
  = link_to 'Back', editable_pages_path
