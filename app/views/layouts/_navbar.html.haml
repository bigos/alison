.navbar.bar
  = link_to 'home', template_index_path
  = link_to 'about me', template_about_me_index_path

  - eps = EditablePage.order(:navbar_index).all
  =# eps.inspect
  - eps[1..-1].each do |ep|
    =   link_to ep.navbar_title,ep


  - if current_admin
    = link_to 'logout','/logout', :id=>'logout'
  - else
    = link_to "login", root_url, :id=>'logout'
